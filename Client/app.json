{
  "expo": {
    "name": "Team 75",
    "slug": "mymapapp",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "extra": {
      "eas": {
        "projectId": "51a3a5ea-77a4-43ea-8813-0a4dbb2d8021"
      }
    },
    "ios": {
      "bundleIdentifier": "com.bhar1gitrrrrrr.mymapapp",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Team 75 to use your location to show your position on the map.",
        "NSLocationAlwaysUsageDescription": "Allow Team 75 to track your shift path in the background.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow Team 75 to track your shift path even when the app is in the background.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "package": "com.bhar1gitrrrrrr.mymapapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCejtHlEF4fn7-2UlGl6OGNaaxkSzHpgOc"
        }
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysPermission": "Allow My Map App to track your shift path in the background."
        }
      ]
    ]
  }
}